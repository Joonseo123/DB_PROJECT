<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!-- snake_case를 camelCase로 자동 매핑 -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
    <!-- 클래스 alise가 들어갈 공간 -->
    
    <typeAlias type = "com.example.demo.dto.FarmDiaryDTO" alias="farmDiary"></typeAlias>
    </typeAliases>
</configuration>